{
  "name": "ic-vui",
  "version": "0.0.9495",
  "description": "A Vue.js UI components lib",
  "main": "lib/ic-vui.js",
  "files": [
    "lib",
    "packages"
  ],
  "author": "chenxianlong <794465731@qq.com>",
  "scripts": {
    "dev": "npm run build:entry && webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "build": "node build/bin/build-lib.js",
    "build:vui": "cross-env NODE_ENV=production webpack --progress --hide-modules --color --config build/webpack.build.js && cross-env NODE_ENV=production webpack -p --progress --hide-modules --color --config build/webpack.build.js",
    "build:entry": "node build/bin/build-entry.js",
    "build:components": "node build/bin/build-components.js",
    "build:style-entry": "node build/bin/build-style-entry.js",
    "build:css": "gulp build --gulpfile packages/theme/gulpfile.js --color",
    "build:pages": "node build/build-dev-pages.js",
    "pub": "npm run build && npm publish --access=public"
  },
